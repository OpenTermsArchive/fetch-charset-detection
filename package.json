{
	"name": "fetch-charset-detection",
	"description": "Charset detection and conversion utilities, originally from node-fetch.",
	"keywords": [
		"content-type",
		"headers",
		"http",
		"meta",
		"node-fetch"
	],
	"version": "0.0.0",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=10"
	},
	"repository": "https://github.com/Richienb/fetch-charset-detection.git",
	"author": "Richie Bendall <richiebendall@gmail.com>",
	"license": "MIT",
	"scripts": {
		"docs": "typedoc",
		"build": "tsc && typedoc",
		"dev": "tsc --watch",
		"lint": "xo",
		"test": "xo && ava"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.3",
		"content-type": "^1.0.4",
		"iconv-lite": "^0.6.2",
		"nice-try": "^2.0.1"
	},
	"devDependencies": {
		"@richienb/tsconfig": "^0.1.1",
		"@richienb/typedoc": "^0.1.1",
		"@types/cheerio": "^0.22.21",
		"@types/content-type": "^1.1.3",
		"@types/nice-try": "^2.0.0",
		"ava": "^3.12.1",
		"eslint-config-richienb": "^0.4.2",
		"node-fetch": "^2.6.0",
		"ts-node": "^9.0.0",
		"typedoc": "^0.19.0",
		"typescript": "^4.0.2",
		"xo": "^0.33.0"
	},
	"xo": {
		"extends": "richienb"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	}
}
