{
    "name": "fetch-charset-detection",
    "description": "Charset detection and conversion utilities, originally from node-fetch.",
    "keywords": [
        "content-type",
        "headers",
        "http",
        "meta",
        "node-fetch"
    ],
    "version": "0.0.0",
    "main": "dist/index.js",
    "files": [
        "src/**/*",
        "dist/**/*"
    ],
    "repository": "https://github.com/Richienb/fetch-charset-detection.git",
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "scripts": {
        "dev": "yarn js --watch",
        "build": "yarn js && yarn docs",
        "js": "tsc",
        "docs": "typedoc --out ./docs --mode file --target ES6 --ignoreCompilerErrors ./src",
        "lint": "xo",
        "test": "ava"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.3",
        "content-type": "^1.0.4",
        "iconv-lite": "^0.5.0",
        "lodash": "^4.17.15",
        "nice-try": "^2.0.0"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.13",
        "@types/content-type": "^1.1.3",
        "@types/lodash": "^4.14.146",
        "ava": "^2.4.0",
        "xo": "^0.25.3",
        "eslint-config-richienb": "^0.2.2",
        "express": "^4.17.1",
        "fetch-blob": "^1.0.4",
        "form-data": "^3.0.0",
        "get-port": "^5.0.0",
        "node-fetch": "^2.6.0",
        "resumer": "^0.0.0",
        "ts-node": "^8.4.1",
        "typedoc": "^0.15.0",
        "typescript": "^3.7.2"
    },
    "resolutions": {
        "eslint": "^6.6.0"
    },
    "xo": {
        "extends": "richienb/node",
        "overrides": [
            {
                "files": "test.js",
                "rules": {
                    "import/default": 0,
                    "node/no-missing-import": 0,
                    "import/no-unresolved": 0,
                    "node/no-unsupported-features/node-builtins": 0
                }
            }
        ]
    },
    "ava": {
        "compileEnhancements": false,
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    },
    "engines": {
        "node": ">=0.12"
    }
}
